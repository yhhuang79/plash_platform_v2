<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="tw.edu.sinica.iis.ants.DB.T_AppUser" table="socialcore.app_user">

		<id column="aid" name="aid" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">app_user_aid_seq</param>
			</generator>
		</id>
		<property name="appid" column="appid" />
		<property name="sid" column="sid" />	
	</class>

</hibernate-mapping>