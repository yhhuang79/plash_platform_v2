<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="tw.edu.sinica.iis.ants.DB.T_TripInfo" table="user_location.trip_info">

		<id column="id" name="id" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">user_location.trip_info_id_seq</param>
			</generator>
		</id>


			
		<property name="userid" column="userid" />
		<property name="trip_id" column="trip_id" />
		<property name="trip_name" column="trip_name" />
		<property name="trip_st_address" column="trip_st_address" />
		<property name="trip_st" column="trip_st" />
		<property name="trip_et" column="trip_et" />
		<property name="trip_length" column="trip_length" />
		<property name="num_of_pts" column="num_of_pts" /> 
		<property name="update_status" column="update_status" />
									

	</class>

</hibernate-mapping>