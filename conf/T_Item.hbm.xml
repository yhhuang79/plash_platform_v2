<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="tw.edu.sinica.iis.ants.DB.T_Item" table="activity.item">

		<id column="id" name="id" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">activity.item_seq</param>
			</generator>
		</id>
		<property name="userid" column="userid" />
		<property name="activityid" column="activityid" />
		<property name="name" column="name" />
		<property name="price" column="price" />
		<property name="assigned" column="assigned" />
		<property name="assigneduserid" column="assigneduserid" />

	</class>

</hibernate-mapping>