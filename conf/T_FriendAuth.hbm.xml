<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="tw.edu.sinica.iis.ants.DB.T_FriendAuth" table="user_location.friend_auth">

		<id column="id" name="id" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">user_location.auth_seq</param>
			</generator>
		</id>
		<property name="userAID" column="&quot;userAID&quot;" />
		<property name="tripID" column="&quot;tripID&quot;" />
		<property name="userBID" column="&quot;userBID&quot;" />
	</class>

</hibernate-mapping>