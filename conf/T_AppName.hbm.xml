<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
 PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="tw.edu.sinica.iis.ants.DB.T_AppName" table="app_name">

		<id column="appname_id" name="appname_id" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">app_name_appname_id_seq</param>
			</generator>
		</id>
		<property name="appid" column="appid" />
		<property name="app_name" column="app_name" />
		<property name="apk_file" column="apk_file" />		
	</class>

</hibernate-mapping>